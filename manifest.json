{
    "name":"David Lynch's Weather Report",
    "description":"Open's the latest episode of David Lynch's Weather Report.",
    "version":"0.1",
    "manifest_version":3,
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_popup":"popup.html"
    },
    "permissions":["tabs", "scripting"]
}